<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.ionic588888" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Busca Sorocaba</name>
  <description>
        An Ionic Framework and Cordova project.
    </description>
  <author email="you@example.com" href="http://example.com.com/">
      Your Name Here
    </author>
  <allow-navigation href="*" />
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="SplashScreenDelay" value="5000"/>
  <preference name="FadeSplashScreenDuration" value="2000"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashShowOnlyFirstTime" value="false" />
  <preference name="ShowSplashScreenSpinner" value="false" />

  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>

  <platform name="android">
    <icon src="resources/android/icon/icon.png" density="ldpi"/>
    <icon src="resources/android/icon/icon.png" density="mdpi"/>
    <icon src="resources/android/icon/icon.png" density="hdpi"/>
    <icon src="resources/android/icon/icon.png" density="xhdpi"/>
    <icon src="resources/android/icon/icon.png" density="xxhdpi"/>
    <icon src="resources/android/icon/icon.png" density="xxxhdpi"/>
    <splash src="resources/android/splash/screen.png" density="land-ldpi"/>
    <splash src="resources/android/splash/screen.png" density="land-mdpi"/>
    <splash src="resources/android/splash/screen.png" density="land-hdpi"/>
    <splash src="resources/android/splash/screen.png" density="land-xhdpi"/>
    <splash src="resources/android/splash/screen.png" density="land-xxhdpi"/>
    <splash src="resources/android/splash/screen.png" density="land-xxxhdpi"/>
    <splash src="resources/android/splash/screen.png" density="port-ldpi"/>
    <splash src="resources/android/splash/screen.png" density="port-mdpi"/>
    <splash src="resources/android/splash/screen.png" density="port-hdpi"/>
    <splash src="resources/android/splash/screen.png" density="port-xhdpi"/>
    <splash src="resources/android/splash/screen.png" density="port-xxhdpi"/>
    <splash src="resources/android/splash/screen.png" density="port-xxxhdpi"/>
  </platform>
</widget>